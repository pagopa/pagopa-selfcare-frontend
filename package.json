{
  "name": "pagopa-selfcare-frontend",
  "version": "0.1.0",
  "homepage": "ui",
  "private": true,
  "scripts": {
    "envconfig": "chmod +x env.sh && ./env.sh",
    "envconfig:local": "export $(grep -v '^#' .env.development.local) && chmod +x env.sh && ./env.sh",
    "clean": "npm-run-all clean:*",
    "prestart": "npm-run-all envconfig",
    "start": "craco start",
    "build": "craco build",
    "clean:build": "rimraf build",
    "test": "craco test -i",
    "test:jest": "yarn run generate && dotenv -e .env.test.local jest --coverage --ci --testResultsProcessor jest-sonar-reporter",
    "test:jest-local": "yarn run generate && dotenv -e .env.test.local jest --coverage --ci",
    "test:craco": "craco test -i",
    "test:coverage": "craco test -i --coverage --ci",
    "test:puppeteer": "cd ./integration-test/ && sh ./run_integration.sh",
    "clean:coverage": "rimraf coverage",
    "eject": "react-scripts eject",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx ",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "generate": "npm-run-all generate:*",
    "clean:api-portal": "rimraf src/api/generated/portal && rimraf openApi/generated",
    "generate:api-portal": "jq 'walk(if type == \"object\" and has(\"parameters\") then .parameters |= map(select(.name != \"X-Request-Id\")) else . end)' ./openApi/portal-api-docs.json > ./openApi/portal-api-docs.json.temp && mv ./openApi/portal-api-docs.json.temp ./openApi/portal-api-docs.json && yarn run clean:api-portal && mkdirp openApi/generated && api-spec-converter --from=openapi_3 --to=swagger_2 openApi/portal-api-docs.json > openApi/generated/portal-swagger20.json && node openApi/scripts/api-portal_fixPreGen.js && node ./node_modules/@pagopa/selfcare-common-frontend/utils/fixSwagger20ArraySchemaDef.js openApi/generated/portal-swagger20.json && gen-api-models --api-spec openApi/generated/portal-swagger20.json --out-dir src/api/generated/portal --no-strict --request-types --response-decoders --client && node openApi/scripts/api-portal_fixPostGen.js"
  },
  "dependencies": {
    "@mui/icons-material": "^5.8.2",
    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "^5.8.2",
    "@mui/system": "^5.8.1",
    "@mui/x-data-grid": "5.17.8",
    "@mui/x-data-grid-generator": "^5.0.1",
    "@mui/x-date-pickers": "^5.0.3",
    "@pagopa/mui-italia": "0.8.12",
    "@pagopa/selfcare-common-frontend": "^1.11.8",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-router-hash-link": "^2.4.5",
    "axios": "^0.23.0",
    "buffer": "^6.0.3",
    "date-fns": "^2.30.0",
    "formik": "^2.2.9",
    "node-fetch": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-number-format": "^5.3.1",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "5.0.1",
    "react-use-scrollspy": "^3.0.1",
    "url": "^0.11.0",
    "@types/node": "^20.9.4"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@craco/craco": "^6.4.3",
    "@pagopa/openapi-codegen-ts": "^13.1.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.0.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "api-spec-converter": "^2.12.0",
    "babel-jest": "^27.2.5",
    "babel-plugin-macros": "^3.1.0",
    "craco-module-federation": "^1.1.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "jest-sonar-reporter": "^2.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "puppeteer": "^21.1.1",
    "puppeteer-extra-plugin-session": "^1.0.1",
    "raw.macro": "^0.4.2",
    "regex-replace": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.6"
  },
  "resolutions": {
    "static-eval": "^2.0.2",
    "yargs": "^17.5.1",
    "jsonpath": "^1.1.1",
    "svgo": "^2.8.0",
    "json-schema-ref-parser": "^7.1.0",
    "z-schema": "^4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
